    "@types/sns-validator": "0.3.0",
    "@types/supertest": "2.0.11",
    "@types/uuid": "8.3.1",
    "@typescript-eslint/eslint-plugin": "4.29.3",
    "@typescript-eslint/parser": "4.29.3",
    "babel-plugin-module-resolver": "4.1.0",
    "eslint": "7.32.0",
